@import 'normalize.css';

body,
html {
    scroll-behavior: smooth;
    min-height: 100vh;
}

body {
    padding-top: 70px;
    background-color: #e8e8e8;
}

a {
    color: inherit;
    cursor: pointer;
    text-decoration: none;
}

img {
    display: block;
    max-width: 100%;
    max-height: 100%;
}
